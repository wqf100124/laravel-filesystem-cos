{
  "name": "i94m/laravel-filesystem-cos",
  "description": "Tencent Cloud COS filesystem for Laravel",
  "type": "library",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "i94m",
      "email": "928988368@qq.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/filesystem": "^9.0|^10.0|^11.0|^12.0",
    "league/flysystem": "^3.0",
    "qcloud/cos-sdk-v5": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "I94m\\LaravelFilesystemCos\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "I94m\\LaravelFilesystemCos\\CosServiceProvider"
      ],
      "aliases": {
        "CosStorage": "I94m\\LaravelFilesystemCos\\Facades\\CosStorage"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}